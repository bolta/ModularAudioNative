@tempo 120

@let `pi`, 3.14159265358979
@let `phase`, (sawOsc + 1) * pi

// スタックオーバーフローを起こす場合は a, b をグローバルに移す
// @let `a`, 1
// @let `b`, 0.9

// http://ngs.no.coocan.jp/doc/wiki.cgi/datapack?page=3%2E4+YM2413%28OPLL%29
// 式 7.3
@let `fm`,
	// スタックオーバーフローを起こす場合は a, b をグローバルに移す
	a = 1;
	b = 0.9;
	do fb <- feedback;
	fbIn = fb->at(0);
	fbOut = fb->at(1);
	(a * sin(phase + b * fbOut)) | fbIn

@instrument ^a, fm * adsrEnv

a o4l8v15
a cdefg2
